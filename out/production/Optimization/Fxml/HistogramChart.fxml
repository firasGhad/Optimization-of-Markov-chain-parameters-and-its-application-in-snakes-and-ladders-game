<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="570.0" prefWidth="737.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
            <BarChart fx:id="NumOfMovesChart" prefHeight="249.0" prefWidth="577.0" title="Number of moves" BorderPane.alignment="CENTER">
              <xAxis>
                <CategoryAxis label="Optimzation number" side="BOTTOM" fx:id="x" />
              </xAxis>
              <yAxis>
                <NumberAxis fx:id="y" label="Number of moves" side="LEFT" />
              </yAxis>
            </BarChart>
         </top>
         <bottom>
            <BarChart fx:id="NumOfMovesChart1" prefHeight="900.0" prefWidth="900.0" title="K moves" BorderPane.alignment="CENTER">
               <xAxis>
                  <CategoryAxis label="Number of moves" side="BOTTOM" fx:id="x21" />
               </xAxis>
               <yAxis>
                  <NumberAxis fx:id="y21" label="Count of simulation" side="LEFT" />
               </yAxis>
            </BarChart>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
